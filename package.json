{
  "name": "grim.lib",
  "version": "1.0.4",
  "description": "Lib with different usefull stuff",
  "main": "dist/grim.lib.common.js",
  "files": [
    "dist",
    "src",
    "index.ts"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-stage-0": "^7.0.0-beta.47",
    "@types/chai": "^4.1.2",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^5.0.0",
    "@types/superagent": "^3.5.8",
    "chai": "^4.1.2",
    "change-case": "^3.0.2",
    "create-banner": "^1.0.0",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.5",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-watch": "^4.3.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "uglify-js": "^3.3.26"
  },
  "scripts": {
    "rebuild": "npm run clear && npm run build:js && npm run compress",
    "clear": "del-cli dist",
    "build:css": "postcss src/index.css -o dist/index.css --no-map",
    "build:js": "rollup -c",
    "compress": "npm run compress:js",
    "compress:css": "postcss dist/grim.lib.css -u cssnano -o dist/grim.lib.min.css --no-map",
    "compress:js": "uglifyjs dist/grim.lib.js -o dist/grim.lib.min.js -c -m --comments /^!/",
    "build-ts": "tsc",
    "git-push-all": "git push origin && git push origin --tags",
    "test": "mocha -r ts-node/register -r ignore-styles -r jsdom-global/register test/**/*.ts"
  },
  "keywords": [
    "lib",
    "js",
    "typescript",
    "es6",
    "defineProperty",
    "define",
    "property",
    "callback",
    "collection",
    "uid",
    "cookie"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:grimwoodent/grim.lib.git"
  },
  "author": "GrimwoodEnt",
  "license": "MIT",
  "homepage": "https://github.com/grimwoodent/grim.lib"
}
